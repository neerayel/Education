<Page x:Class="Lab_6.Page2"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Lab_6"
      mc:Ignorable="d" 
      d:DesignHeight="460" d:DesignWidth="460"
      Title="Page2">

    <Grid>
        <TextBox HorizontalAlignment="Left" x:Name="TB_Result" FontSize="16" IsReadOnly="True" Margin="80,130,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="300" Height="150"/>
        <TextBox HorizontalAlignment="Left" x:Name="TB_Result2" FontSize="16" IsReadOnly="True" Margin="80,320,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="300" Height="50"/>
        <TextBox HorizontalAlignment="Left" Text="max" x:Name="TB_1" Margin="80,40,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="50" Height="30"/>
        <TextBox HorizontalAlignment="Left" Text="n" x:Name="TB_2" Margin="165,40,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="50" Height="30"/>
        <Button Content="Process" HorizontalAlignment="Left" Margin="350,40,0,0" VerticalAlignment="Top" Height="30" Width="60" Click="Button_Click"/>
    </Grid>
</Page>
