<Page x:Class="Lab_6.Page3"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Lab_6"
      mc:Ignorable="d" 
      d:DesignHeight="460" d:DesignWidth="460"
      Title="Page3">

    <Grid>
        <TextBox HorizontalAlignment="Left" x:Name="TB_Result" FontSize="16" IsReadOnly="True" Margin="80,100,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="300" Height="235"/>
        <TextBox HorizontalAlignment="Left" x:Name="TB_Result2" FontSize="16" IsReadOnly="True" Margin="80,350,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="300" Height="50"/>
        <TextBox HorizontalAlignment="Left" Text="max" x:Name="TB_1" Margin="80,40,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="50" Height="30"/>
        <TextBox HorizontalAlignment="Left" Text="кол-во столбцов" x:Name="TB_2" Margin="154,40,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="60" Height="40"/>
        <TextBox HorizontalAlignment="Left" Text="кол-во строк" x:Name="TB_3" Margin="250,40,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="60" Height="40"/>
        <Button Content="Process" HorizontalAlignment="Left" Margin="350,40,0,0" VerticalAlignment="Top" Height="30" Width="60" Click="Button_Click"/>
    </Grid>
</Page>
